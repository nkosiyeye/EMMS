@model IEnumerable<EMMS.Models.Entities.LookupList>

<a style="color:black" href="#lookupmenu" data-bs-toggle="collapse" class="list-group-item border-0 d-inline-block text-truncate" data-bs-parent="#sidebar">
    <i class="fa-solid fa-list"></i> <span>Lookups</span>
</a>
<div style="border:solid; border-color:gray;" id="lookupmenu" class="collapse list-group border-1 rounded-0 text-sm-start">
    @foreach (var lookup in Model)
    {
        <a style="font-size:0.8rem;font-weight:600;"
           asp-controller="Admin"
           asp-action="Index"
           asp-route-id="@lookup.LookupListId"
           class="list-group-item border-0 d-inline-block text-truncate"
           data-bs-parent="#sidebar">
            <span>@lookup.Name</span>
        </a>
    }
</div>